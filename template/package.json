{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    {{#if_eq runner "jest"}}
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    {{/if_eq}}
    {{#if_eq runner "karma"}}
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    {{/if_eq}}
    {{#e2e}}
    "e2e": "node test/e2e/runner.js",
    {{/e2e}}
    {{#if_or runner e2e}}
    "test": "{{#runner}}npm run unit{{/runner}}{{#runner}}{{#e2e}} && {{/e2e}}{{/runner}}{{#e2e}}npm run e2e{{/e2e}}",
    {{/if_or}}
    {{#lint}}
    "lint": "eslint --ext .js,.vue src{{#runner}} test/unit{{/runner}}{{#e2e}} test/e2e/specs{{/e2e}}",
    {{/lint}}
    "build": "node build/build.js",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev"
  },
  "dependencies": {
    {{#vuex}}
    "vuex": "^3.0.1",
    {{/vuex}}
    {{#router}}
    {{#if_not mpvue}}
    "vue-router": "^3.0.1",
    {{/if_not}}
    {{#mpvue}}
    "mpvue-router-patch": "^0.2.1",
    {{/mpvue}}
    {{/router}}
    {{#mpvue}}
    "mpvue": "^1.0.13",
    {{else}}
    "vue": "^2.5.17"
    {{/mpvue}}
    "@ddjf/ddui":"^1.0.1",
    "@ddjf/mpweui": "^1.0.0",
    "weui.js": "^1.1.4",
    {{#if_not mpvue}}
    "@ddjf/weui": "^1.0.0",
    {{/if_not}}
    "flyio": "^0.6.0",
    "vue": "^2.5.2",
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    {{#lint}}
    "babel-eslint": "^7.2.3",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    {{#if_eq lint "standard"}}
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    {{/if_eq}}
    {{#if_eq lint "airbnb"}}
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    {{/if_eq}}
    {{/lint}}
    {{#if_eq runner "jest"}}
    "babel-jest": "^21.0.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "vue-jest": "^1.0.2",
    {{/if_eq}}
    {{#if_eq runner "karma"}}
    "cross-env": "^5.0.1",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "chai": "^4.1.2",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.8.0",
    "inject-loader": "^3.0.0",
    "babel-plugin-istanbul": "^4.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    {{/if_eq}}
    {{#e2e}}
    "babel-register": "^6.22.0",
    "chromedriver": "^2.27.2",
    "cross-spawn": "^5.0.1",
    "nightwatch": "^0.9.12",
    "selenium-server": "^3.0.1",
    {{/e2e}}
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.4.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    {{#if_not mpvue}}
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "webpack-dev-server": "^2.9.1",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    {{/if_not}}
    {{#mpvue}}
    "mpvue-entry": "^1.5.1",
    "mpvue-loader": "^1.1.4",
    "mpvue-template-compiler": "^1.0.13",
    "mpvue-webpack-target": "^1.0.1",
    "postcss-mpvue-wxss": "^1.0.0",
    "px2rpx-loader": "^0.1.10",
    "webpack-mpvue-asset-plugin": "^0.1.0",
    {{/mpvue}}
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "node-notifier": "^5.1.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "quick-local-ip": "^1.0.7",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "sass-rem": "^2.0.1",
    "sass-resources-loader": "^1.3.3",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^2.0.0",
    "rimraf": "^2.6.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^0.5.8",
    "vue-style-loader": "^4.1.2",
    "portfinder": "^1.0.13",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware-hard-disk": "^1.12.0",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
